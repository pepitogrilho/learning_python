    "#Pure: it has always the same result \n",
    "def pure_function(x,y):\n",
    "    return (x+2*y)/(2*x+y)\n",
    "print(pure_function(1,1))\n",
    "#Impure: the result depends on external conditions\n",
    "list_01 = []\n",
    "def impure_function(arg):\n",
    "    list_01.append(arg)  \n",
    "impure_function(\"kk\")\n",
    "print(list_01)"
